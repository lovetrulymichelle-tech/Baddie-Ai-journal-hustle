[flake8]
max-line-length = 127
max-complexity = 10
exclude = 
    venv,
    .venv,
    env,
    .env,
    __pycache__,
    .git,
    build,
    dist,
    *.egg-info,
    .tox,
    .pytest_cache
ignore = 
    E203,
    W503,
    E501
per-file-ignores =
    __init__.py:F401,F403
    database.py:C901
    demo.py:C901
    migrate_sqlite_to_postgres.py:C901